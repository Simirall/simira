{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "noEmit": true,
    "isolatedModules": true,
    "moduleResolution": "Bundler",
    "moduleDetection": "force",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "lib": ["ESNext", "DOM"],
    "jsx": "react-jsx"
  },
}
